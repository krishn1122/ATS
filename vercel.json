{
  "version": 2,
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/frontend/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "BACKEND_URL": "https://ats-production-4ee3.up.railway.app"
  }
}