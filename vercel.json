{
  "version": 2,
  "builds": [
    {
      "src": "frontend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/app.py"
    }
  ],
  "env": {
    "BACKEND_URL": "https://your-backend-service.railway.app"
  },
  "functions": {
    "frontend/app.py": {
      "runtime": "python3.9"
    }
  }
}